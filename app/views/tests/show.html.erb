<div class="container">

<h1 align="center"><%= @test.Test_name %></h1>
<div class="well">
  <%= @test.Test_desc %><br><br>
  Time limit: <%= @test.Test_timelimit %> min
</div>


<% @test.subjects.each do |s|%>
<div class="col-md-4">
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><%= link_to "#{s.Subject_name}", subject_path(s)%></h3>
  </div>
  <div class="panel-body">
    Panel content
  </div>
</div>
</div>
<% end %>


</div>
