<div class="container">

<h1 align="center">Tests</h1>

<% @test.each do |ts| %>
<div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title"><%= link_to "#{ts.Test_name}",test_path(ts)%></h3>
  </div>
  <div class="panel-body">
    <%= ts.Test_desc %>
  </div>
  <div class="panel-footer">Time limit: <%=ts.Test_timelimit %> min</div>
</div>
<% end %>

<%= link_to "Home", root_path, class: "btn" %>
</div>
